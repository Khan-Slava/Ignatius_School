@{
    Layout = "_Layout";
}

<section class="section">
    <main class="hero-content">
        <div class="w-layout-blockcontainer container hero w-container">
            <div class="hero-section-content">
                <div class="hero-section-content-block home">
                    <div class="wrapper max-width-640-mobile-480">
                        <h1 class="h1 margin-bottom-28">Empowering Minds, Inspiring Futures</h1>
                        <p class="regular-xl max-width-420">Welcome to St. Ignatius College School! Get ready for another year of learning and growing together. We&#x27;re excited to have you here!</p>
                    </div>
                    <div class="buttons-wrapper">
                        <a href="/about" class="button primary w-button">About School</a>
                        <a href="/contact-us" class="button secondary w-button">Contact Us</a>
                    </div>
                </div>
                <div class="hero-section-image home-block">
                    <div class="hero-home-collection-list-wrapper w-dyn-list">
                        <div role="list" class="hero-home-collection-list w-dyn-items">
                            <div role="listitem" class="hero-home-collection-item w-dyn-item">
                                <a href="/news-events/pa-day-for-elementary-and-secondary-schools" class="hero-home-collection-item-content w-inline-block">
                                    <div class="wrapper">
                                        <div class="regular-m grey-80 margin-bottom-16">Mar 3</div>
                                        <h3 class="h3">PA Day for Elementary and Secondary Schools</h3>
                                    </div>
                                    <div class="tertiary-button">
                                        <div class="regular-m">Discover Event</div>
                                        <div class="tertiary-button-icon">
                                            <img src="images/arrow-20right.svg" loading="lazy" alt="" class="icon-size-14" />
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>
</section>
<section class="section margin-top-64-tablet-48-mobile-24">
    <div class="w-layout-blockcontainer container w-container">
        <div class="banner-type-one">
            <div class="banner-type-one-content">
                <div class="wrapper max-width-880-tablet-640-mobile-480">
                    <p class="banner-heading">Join Our Welcoming Community through St. Ignatius College School Admissions</p>
                </div>
